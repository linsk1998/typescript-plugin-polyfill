{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Mocha Current Test File",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"${file}",
				"--timeout",
				"9999"
			],
			"internalConsoleOptions": "openOnSessionStart",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/src/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Node Run Current File",
			"program": "${file}"
		}
	]
}
